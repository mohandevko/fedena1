<%= form_for(@wk) do |f| %>
  User_id: <%= f.text_field :user_id, :value => @user.id %> <br />
  Job Title: <%= f.text_field :title %> <br />
  business/Company Name: <%= f.text_field :company %> <br />
  Description: <%= f.text_area :description, :rows => 5 %> <br />
  Start_date: <%= f.text_field :start_date %> <br />
  End_date: <%= f.text_field :end_date %> <br />
  <%= f.submit %>

<% end %>

